{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "strict": true,
    "outDir": "./bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": "./k21",
    "paths": {
      "@k21": [
        "./"
      ],
      "@k21/*": [
        "./*"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "./bin",
    "./jest.config.ts"]
}
