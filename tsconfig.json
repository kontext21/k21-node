{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "outDir": "./bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": "./k21",
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "bench", "__test__"]
}
